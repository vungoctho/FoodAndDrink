@using ClientDependency.Core.Mvc
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="@ViewBag.Description">
    <title>@ViewBag.Title</title>
    <link href="http://fonts.googleapis.com/css?family=Rock+Salt" rel="stylesheet" type="text/css">
    <link href="/favicon.ico?v=1" rel="shortcut icon" type="image/x-icon">
    
    <link rel="stylesheet" href="~/Assets/vendor/normalize.css">
    <link rel="stylesheet" href="~/Assets/vendor/foundation/foundation.css">
    <link rel="stylesheet" href="~/Assets/vendor/foundation/foundation-icons.css">
    <link rel="stylesheet" href="~/Assets/vendor/quill/quill.bubble.css">
    <link rel="stylesheet" href="~/Assets/vendor/font-awesome/css/font-awesome.css">
        
    @Html.RenderCssHere()
    @RenderSection("styles", false)
    <script src="~/Assets/vendor/Scripts/modernizr.js"></script>
    <script src="~/Assets/vendor/Scripts/jquery.min.js"></script>
    <script src="~/Assets/vendor/Scripts/foundation.min.js"></script>
    <script src="~/Assets/vendor/Scripts/angular.js"></script>
    <script src="~/Assets/vendor/Scripts/angular-sanitize.min.js"></script>
    <script src="~/Assets/vendor/underscore/underscore-min.js"></script>

    <link rel="icon" href="~/Assets/vendor/zGourmet/images/favicon.ico">
    <link rel="shortcut icon" href="~/Assets/vendor/zGourmet/images/favicon.ico" />
    <link rel="stylesheet" href="~/Assets/vendor/zGourmet/css/style.css">
    <link rel="stylesheet" href="~/Assets/vendor/zGourmet/css/slider.css">
    <link rel="stylesheet" href="~/Assets/vendor/zGourmet/css/zerogrid.css" type="text/css" media="screen">
    <link rel="stylesheet" href="~/Assets/vendor/zGourmet/css/responsive.css" type="text/css" media="screen">    
    <script src="~/Assets/vendor/zGourmet/js/jquery-migrate-1.1.1.js"></script>
    <script src="~/Assets/vendor/zGourmet/js/superfish.js"></script>
    <script src="~/Assets/vendor/zGourmet/js/jquery.easing.1.3.js"></script>
    <script src="~/Assets/vendor/zGourmet/js/sForm.js"></script>
    <script src="~/Assets/vendor/zGourmet/js/jquery.carouFredSel-6.1.0-packed.js"></script>
    <script src="~/Assets/vendor/zGourmet/js/tms-0.4.1.js"></script>
    <script src="~/Assets/vendor/zGourmet/js/css3-mediaqueries.js"></script>

    @{
        Html.RequiresJs("~/Assets/vendor/Scripts/quill.min.js")
            .RequiresJs("~/Assets/js/inline-richtext-manager.js");
    }
</head>
<body>
    @RenderBody()
    @Html.RenderJsHere()
    @RenderSection("scripts", false)
</body>
</html>